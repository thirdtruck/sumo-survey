<div class="row">
    <div class="col-md-12">
        <div data-question-id="{{question.id}}" class="question">
            <h3>Survey Question</h3>
            <p>{{question.title}}</p>
            {{#each question.choices}}
                <a data-choice-id="{{id}}" class="btn btn-primary" href="#">{{text}}</a>
            {{/each}}
        </div>
    </div>
</div>
<!-- /.row -->
